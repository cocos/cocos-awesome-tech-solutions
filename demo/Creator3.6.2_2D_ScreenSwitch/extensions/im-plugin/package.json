{
  "name": "im-plugin",
  "version": "3.0.10",
  "package_version": 2,
  "description": "i18n:im-plugin.title",
  "author": "Cocos Creator",
  "main": "./src/browser.js",
  "panels": {
    "default": {
      "title": "i18n:im-plugin.title",
      "main": "./src/panel/default/index.js",
      "type": "dockable",
      "size": {
        "width": 1000,
        "height": 700,
        "min-width": 720,
        "min-height": 480
      },
      "icon": "./resources/images/icon.png"
    }
  },
  "contributions": {
    "messages": {
      "show-panel": {
        "methods": [
          "showPanel"
        ]
      },
      "silent-check-and-update": {
        "methods": [
          "silentCheckAndUpdate"
        ]
      },
      "im-plugin:check": {
        "methods": [
          "default.check"
        ]
      },
      "silent-check": {
        "methods": [
          "silentCheck"
        ]
      },
      "close-panel": {
        "methods": [
          "closePanel"
        ]
      },
      "whether-close": {
        "methods": [
          "whetherClose"
        ]
      },
      "get-sign-in-url": {
        "methods": [
          "getSignInUrl"
        ]
      },
      "get-redirect-url": {
        "methods": [
          "getRedirectUrl"
        ]
      },
      "update-toolbar": {
        "methods": [
          "updateToolbarMain"
        ]
      },
      "save-toolbar-info": {
        "methods": [
          "saveToolbarInfo"
        ]
      },
      "get-saved-toolbar-info": {
        "methods": [
          "getSavedToolbarInfo"
        ]
      }
    },
    "menu": [
      {
        "path": "i18n:menu.panel",
        "label": "i18n:im-plugin.title",
        "message": "show-panel",
        "icon": "./resources/images/icon.png"
      }
    ],
    "toolbar": {
      "right": "./src/toolbar/toolbar.js",
      "rightPriority": -1
    }
  },
  "scripts": {
    "build": "gulp",
    "clean": "gulp clean",
    "watch": "gulp && gulp watch",
    "release": "cross-env NODE_ENV=production gulp && node ./pack.js"
  },
  "dependencies": {
    "@marcopeg/template": "^1.12.0",
    "@types/fs-extra": "^9.0.12",
    "axios": "^0.23.0",
    "compare-versions": "^3.6.0",
    "dayjs": "^1.10.7",
    "download": "^8.0.0",
    "file-url": "^3.0.0",
    "fix-path": "^4.0.0",
    "fs-extra": "^10.0.0",
    "graceful-fs": "^4.2.8",
    "public-ip": "^4.0.4",
    "query-string": "^6.13.1",
    "request": "^2.88.2",
    "request-progress": "^3.0.0"
  },
  "devDependencies": {
    "@types/download": "^6.2.4",
    "@types/electron": "^1.6.10",
    "@types/gulp": "^4.0.7",
    "@types/gulp-filter": "^3.0.33",
    "@types/gulp-sourcemaps": "^0.0.33",
    "@types/gulp-terser": "^1.2.1",
    "@types/md5": "^2.2.1",
    "@types/node": "^14.12.10",
    "@types/request": "^2.48.5",
    "archiver": "^5.0.0",
    "cross-env": "^7.0.3",
    "del": "^6.0.0",
    "gulp": "^4.0.2",
    "gulp-filter": "^6.0.0",
    "gulp-if": "^3.0.0",
    "gulp-javascript-obfuscator": "^1.1.6",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-terser": "^2.0.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "gulp-yarn": "^2.0.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.1.2"
  }
}
